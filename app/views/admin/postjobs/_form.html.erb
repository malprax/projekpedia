<%= simple_form_for([:admin,@postjob]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :content %>
    <%= f.input :client_id %>
    <%= f.input :timestamp %>
    <%= f.input :price %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class:'btn btn-success' %>&nbsp;&nbsp;<%= link_to 'Back', admin_postjobs_path, class: 'btn btn-primary'%>
  </div>
<% end %>
